@import 'nib'
@import 'https://fonts.googleapis.com/css?family=Roboto:400,900&subset=cyrillic';
//font-family: 'Roboto', sans-serif;
@import 'https://fonts.googleapis.com/css?family=PT+Serif:400,400i,700&subset=cyrillic';
//font-family: 'PT Serif', serif;

*
	box-sizing border-box
.clearfix
	clearfix()          
body
	font-family 'PT Serif', serif
	color #000
	font-size 17px
	background #fff
	position relative
	max-width 1920px
	min-width 320px
	padding 0
	margin 0
	clearfix()
*
	box-sizing border-box
a
	text-decoration none
img
	max-width 100%
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
	
.wrap
	width 89.5%
	margin 0 auto
	clearfix()
header
	background url("../images/main-bg.png") no-repeat 100% 15% / contain
	&.inner
		background none
.top-line
	clearfix()
	padding-top 35px
	.mobile-menu
		display none
	.logo
		float left
		a
			font-size 30px
			color #6b6b6b
	nav
		text-align center
		padding-top 12px
		.close-nav
			display none
		ul
			display inline-block
			padding 0
			margin 0
			li
				list-style-type none
				display inline-block
				a
					font-size 15px
					color #000
					padding 0 10px
					transition 0.3s
					font-family: 'Roboto', sans-serif;
					&:hover
						color #e9366c
						text-decoration underline
		a.cb
			color #40bc98
			font-size 17px
			border-bottom 1px dashed #40bc98
			margin-left 3.4%
			&:hover
				border-bottom 0
	.top-phone
		float right
		div
			font-weight 700
			font-size 30px
			margin-bottom 5px
		span
			font-style italic
			color #686868
.main-line
	padding-top 70px
	clearfix()
	.main-title
		float left
		max-width 32%
		position relative
		.slogan
			margin-bottom 40px
			h1
				font-size 60px
				margin 0
			span
				font-style italic
				font-size 33px
				color #6b6b6b
		p
			color #7f7f7f
			font-style italic
			margin 0
		.common-price
			margin-top 30px
			font-style italic
			font-size 25px
			b
				color #e9366c
		&:after
			display block
			content url("../images/man.png")
			position absolute
			top -50px
			right -90%
	.main-form
		float right
		width 537px
		// height 462px
		background #f7f7f7
		border 1px solid #e8e8e8
		border-radius 5px
		z-index 2
		position relative
		padding 30px 35px
		.title
			font-size 25px
		form
			display block
			margin 20px 0
			.form-field
				clearfix()
				&:not(:last-child)
					margin-bottom 10px
				&:last-child
					padding-left 155px
				.field-name
					float left
					width 155px
					font-size 15px
					color #5f5f5f
					text-align right
					padding-right 12px
					line-height 44px
				.field-input
					float left
					width 293px
					input,
					select
						display block
						height 44px
						background #fff
						border-radius 2px
						outline none
						border 1px solid #cbcbcb
						padding-left 15px
						-moz-appearance: none;
					input.type-input						
						width 100%
					input.date-range
						float left
						width 142px
						&:first-child
							margin-right 9px
					select
						width 142px
						-webkit-appearance none
						background url("../images/select-arrow.png") no-repeat 90% center, #fff
				input.send-btn
					width 178px
					height 41px
					line-height 40px
					border-radius 5px
					background #e9366c
					text-align center
					cursor pointer
					transition 0.3s
					font-size 14.26px
					font-family: 'Roboto', sans-serif;
					font-weight 900
					border none
					color #fff
					display block
					margin 15px 0 20px 0
					appearance none
					&:hover
						background #b71d4c
					&+span
						font-style italic
						color #686868
section.open-rooms
	margin-top 80px
	.wrap
		padding-bottom 100px
		.open-rooms-title
			position relative
			width 100%
			clearfix()
			&:after
				display block
				position absolute
				top 25px
				width 100%
				right 0
				height 1px
				background #d0d0d0
				content ""
				z-index -1
			div
				background #fff
				font-style italic
				font-size 33px
				padding-right 25px
				float left
		.room-types
			clearfix()
			width 85%
			margin 90px auto 0
			.room-item
				width 25%
				float left
				position relative
				padding-left 45px
				&:last-child
					padding-left 75px
				.places-type
					position absolute
					top 0
					left 0
					font-size 60px
				.places
					font-size 33px
					padding-top 12px
					color #6b6b6b
					font-style italic
					margin-bottom 10px
				.places-count
					color #020202
					font-style italic
					border-bottom 1px solid #a6b8b6
					display inline-block
				.places-price
					font-style italic
					color #e9366c
section.work-stat
	margin-bottom 120px
	background url('../images/bg2.png') no-repeat left center
	.wrap
		.title
			text-align center
			font-size 60px
			line-height 120px
			background url("../images/title-bg.png") no-repeat center
			margin-bottom 200px
			b
				color #e9366c
		.stat
			font-size 33px
			font-style italic
			padding-left 29%
			b
				color #e9366c

.adv-line
	clearfix()
	margin-bottom 30px
	.adv-item
		position relative
		width 20%
		float left
		text-align center
		padding-top 90px
		img
			display block
			position absolute
			top 0
			left 50%
			transform translateX(-50%)
		div
			color #515151
			max-width 165px
			margin 0 auto
.more-btn
	text-align center
	position relative
	&:after
		display block
		position absolute
		top 20px
		width 100%
		right 0
		height 1px
		background #d0d0d0
		content ""
		z-index -1
	.btn-container
		display inline-block
		padding 0 65px
		background #fff
		a
			display inline-block
			line-height 40px
			border-radius 5px
			background #e9366c
			text-align center
			cursor pointer
			transition 0.3s
			font-size 14.26px
			font-family: 'Roboto', sans-serif;
			font-weight 900
			color #fff
			padding 1px 24px
			&:hover
				background #b71d4c
section.hostel-photos
	padding-top 75px
	.wrap
		h2
			font-size 33px
			font-style italic
			margin-bottom 35px
			text-align center
		.photos-line
			margin-bottom 35px
			clearfix()
			.item-photo
				overflow hidden
				width 20%
				float left
				position relative
				img
					display block
					width 100%
				&:hover
					.photo-description
						bottom 0
.photo-description
	position absolute
	bottom -100%
	left 0
	width 100%
	height 100%
	// padding 45% 5% 0
	background rgba(0,0,0,.5)
	transition 0.5s
	.descriptiona-data
		position absolute
		left 5%		
		width 90%
		bottom 10%
		.room-type
			font-style italic
			color #fff
			display inline-block
			border-bottom 1px solid #a6b8b6
		.room-price
			font-weight 700
			font-style italic
			color #e9366c
			margin-top 5px
			margin-bottom 10px
		a
			font-size 14.26px
			color #fff
			cursor pointer
			transition 0.3s
			border-radius 5px
			background #e9366c
			font-family 'Roboto', sans-serif
			font-weight 400
			padding 2% 5%
			&:hover
				background #b71d4c
section.response
	.response-wrap
		padding 80px 0
		max-width 850px
		margin 0 auto
		h3
			font-size 59px
			&+span
				font-style italic
				color #020202
		.response-slider
			padding 40px 0 0 120px
			position relative
			margin-top 70px
			&:before
				display block
				content url("../images/bag.png")
				position absolute
				top 0
				left -25px
			.response-item
				clearfix()
				.author-photo
					float left
					width 207px
					min-height 100%
					margin-right 30px
					position relative
					padding-top 20px
					z-index 1
					img
						display block
						width 100%
				.response-container
					font-style italic
					float left
					max-width 490px
					.response-data
						&:after
							display block
							content ''
							clear both
						.author
							font-size 33px
							position relative
							float left
						.date
							font-size 14.26px
							color #e9366c
							float left
							margin-left 15px
							font-style normal
							font-family 'Roboto', sans-serif
					.response-text
						margin 20px 0
						color #7f7f7f
footer
	.wrap
		border-top 3px solid #d0d0d0
		.footer-wrap
			max-width 1050px
			margin 0 auto
			padding 45px 0
			clearfix()
			.footer-mail,
			.worktime
				float left
				color #8d8d8d
				font-family 'Roboto', sans-serif
				padding 15px 0
			.footer-mail
				float left
				text-align right
			.worktime
				float right
			.adress
				text-align center
				.phone
					font-weight 700
					font-size 30px
					margin-bottom 5px
				.location
					font-style italic
					color #686868
				a
					color #000
section.breadcrumbs
	.wrap
		.nav-links
			a,
			span
				font-size 13px
				font-family 'Roboto', sans-serif
				color #e9366c
				text-decoration underline
				&.current
					text-decoration none
					color #000
				&:hover
					text-decoration none
				&:not(:last-child)
					&:after
						content '/'
						display inline-block
						margin 0 5px
						color #000
						text-decoration none
section.content
	.wrap
		h1
			margin 60px 0 40px 0
			font-size 60px
		.rooms-container
			padding-left 100px
			.room
				position relative
				margin-bottom 100px
				.room-type
					font-size 33px
					font-weight 700
					color #40bc98
					border-bottom 1px solid #afafaf
					margin-bottom 20px
				.room-data
					clearfix()
					.about-room
						max-width 90%
						float left
						clearfix()
						.room-preview
							float left
							width 418px
							margin-right 35px
							img
								display block
								width 100%
							a.all-photos
								display none
						.room-equip
							float left
							margin-right 75px
							.title
								font-size 25px
								margin-bottom 20px
							ul
								padding 0
								margin 0
								width 365px
								li
									list-style-type none
									position relative
									background url("../images/dot.png") repeat-x 10% 75%
									clearfix()
									&:before
										position absolute
										top 10px
										left 0
										width 6px
										height 6px
										border-radius 50%
										background #e9366c
									// &:after
									// 	display block
									// 	// width 100%
									// 	// height 1px
									// 	content ""
									// 	border-bottom 1px dashed #7f7f7f
									div
										font-style italic
										color #7f7f7f
										background #fff
										&:first-child
											float left
											padding-left 15px
											padding-right 5px
										&:last-child
											float right
											padding-left 5px
									&:not(:last-child)
										margin-bottom 10px
						.info
							float left
							clearfix()
							.additional-info
								max-width 418px
								padding-top 55px
								margin-bottom 80px
								.title
									font-style italic
									display inline-block
									border-bottom 1px solid #a6b8b6
								p
									font-size 15px
									color #444444
									margin 5px 0 0 0
									font-style italic
							.room-price
								.price
									clearfix()
									margin-bottom 15px
									span
										font-size 25.05px
										margin-right 20px
									b
										font-size 30px
										font-weight 700
										color #e9366c
										font-style italic
								.order-room
									clearfix()
									a
										font-family "Roboto", sans-serif
										font-weight 900
										cursor pointer
										transition 0.3s
										border-radius 5px
										color #fff
										background #e9366c
										padding 11px 12px
										margin-right 15px
										display block
										float left
										&:hover
											background #b71d4c
									span
										font-style italic
										color #686868
					.room-status
						margin-top 45px
						float right
						max-width 10%
						div
							padding-top 70px
							max-width 140px
						.open
							background url("../images/open-tag.png") no-repeat left top
						.close
							background url("../images/close-tag.png") no-repeat left top	
		
		.callback-info
			padding-top 50px
			margin-bottom 110px
			text-align center
			div:first-child
				font-size 33px
				color #909090
			div:nth-child(2)
				font-size 59px
				color #000
			div:nth-child(3)
				font-size 33px	
		.adv-line			
			max-width 1400px
			margin 0 auto 50px
		.more-advantages
			max-width 50%
			position relative
			margin-bottom 60px
			&+img
				display none
			&:after
				display block
				content url("../images/girl.png")
				position absolute
				top 50px
				right -90%
			div
				font-size 59.80px
				color #000
				margin-bottom 50px
			ul
				padding 0
				margin 0
				li
					list-style-type none
					padding-left 20px
					font-style italic
					font-size 20px
					position relative
					&:not(:last-child)
						margin-bottom 25px
					&:before
						display block
						content ''
						background #e9366c
						width 13px
						height 13px
						-webkit-border-radius 7px 6px 6px 7px/7px 7px 6px 6px
						position absolute
						top 8px
						left 0	
		article
			padding-left 5vw
			max-width 1160px
			.subtitle
				font-size 25px
				color #000
				margin-bottom 10px
			p
				color #7f7f7f
				font-size 17px
				font-style italic
				margin 0 0 30px 0
			figure
				margin 0 0 35px 0
				figcaption
					font-size 17px
					color #000
					font-style italic
		.rooms-slider
			position relative
			margin-bottom 60px
			.slider-name
				font-style italic
				color #000
				margin-bottom 10px
			.slider-nav
				.left-arrow,
				.right-arrow
					position absolute
					cursor pointer
					top 54%
					width 11px
					height 19px
				.left-arrow
					background url("../images/slider-arrows.png") no-repeat 0px 0px
					left -40px
				.right-arrow
					background url("../images/slider-arrows.png") no-repeat 0px -19px
					right -40px
			.slider-wrap
				position relative
				clearfix()
				.slider-item
					overflow hidden
					float left
					// max-width 33.25%
					img
						display block
					&:not(:last-child)
						margin-right 1px
					.item
						position relative
						&:not(:last-child)
							margin-bottom 1px
						.photo-description
							bottom -101%
						&:hover
							.photo-description
								bottom 0
		.contacts-data
			clearfix()
			width 76%
			margin 0 auto
			.data-item
				max-width 35%
				float left
				&:not(:first-child)
					min-height 122px
				&:not(:last-child)
					margin-right 50px
				div
					font-size 25px
					color #000
				p
					margin 0 0 10px 0
					font-style italic
					color #7f7f7f
		.map-container
			padding 0 12%
			background url("../images/left-hand.png") no-repeat 6.3% 100%, url("../images/right-hand.png") no-repeat 93% 100%
			background-size 12%
			margin 0 auto
			padding-bottom 80px
			position relative
			// &:before,
			// &:after
			// 	display block
			// 	content url("../images/left-hand.png")
			// 	position absolute
			// 	bottom -5px
			// &:before
			// 	content url("../images/left-hand.png")
			// 	left -8.3%
			// &:after				
			// 	content url("../images/right-hand.png")				
			// 	right -7.8%
			.title
				font-size 25px
				color #000
				margin-bottom 15px
			.map
				padding 15px
				border 5px solid #000
				border-radius 3px
				z-index -1
				position relative
.popup
	display none
	width 100%
	text-align center
	padding 105px 0
	position absolute
	top 0
	left 0
	z-index 10
	background rgba(#a8e5c4, .95)
	.close-btn
		position absolute
		top 30px
		right 30px
		cursor pointer
	.callback-modal
		display inline-block
		margin 0 auto
		padding 35px 50px
		border-radius 5px
		background #f7f7f7
		box-shadow 0 23px 38px rgba(75,130,100,.81)
		border solid 1px #e8e8e8
		z-index 10
		&>div
			font-size 25px
			color #000
			text-align center		
			margin-bottom 30px
		.callback-form
			.form-field
				clearfix()
				&:first-child
					margin-bottom 10px
				&:nth-child(2)
					margin-bottom 27px
				&:nth-child(3)
					padding-left 140px
				&>div
					width 140px
					float left
					text-align right
					padding-right 12px
					line-height 44px
					font-size 15px
					color #5f5f5f
				input[type="text"]
					display block
					float left
					width 293px
					height 44px
					background #fff
					outline none
					border-radius 2px
					border 1px solid #cbcbcb
					font-size 17px
					color #000
					padding 0 15px
				input[type='submit']
					-moz-appearance: none;
					width 178px
					height 41px
					line-height 40px
					border-radius 5px
					background #e9366c
					text-align center
					cursor pointer
					transition 0.3s
					font-size 14.26px
					font-family: 'Roboto', sans-serif;
					font-weight 900
					border none
					color #fff
					display block
					-webkit-appearance none
					&:hover
						background #b71d4c
					&+span
						font-style italic
						color #686868
						font-size 17px
@media screen and (max-width: 1890px)
	section.content .wrap .rooms-container .room .room-data .about-room .info .additional-info
		float left
		margin-right 35px		
		padding-right 10%
		margin-bottom 0
	section.content .wrap .rooms-container .room .room-data .about-room .info .additional-info + .room-price
		float left
		margin-top 70px
	section.content .wrap .rooms-container .room .room-data .about-room .room-equip
		margin-right 0
@media screen and (max-width: 1440px)
	.main-line .main-title
		max-width 40%
	.main-line .main-title .slogan h1
		font-size 52px
	.main-line .main-title:after
		right -60%
	section.content .wrap .more-advantages
		max-width 55%
	section.content .wrap .more-advantages:after
		right -80%
	section.content .wrap .more-advantages ul li:not(:last-child)
		margin-bottom 20px
@media screen and (max-width: 1365px)
	.wrap
		width 94%
	.main-line .main-title:after
		display none
	.main-line .main-title .slogan
		margin-bottom 20px
	.main-line .main-title .slogan h1
		font-size 38px
	.main-line .main-title .slogan span
		font-size 28px
		line-height 24px
	section.open-rooms .wrap .room-types
		width 100%
	section.open-rooms .wrap .room-types .room-item .places
		font-size 28px
	section.open-rooms .wrap .room-types .room-item .places-count
		font-size 14px
	section.work-stat .wrap .stat
		padding-left 0
		text-align center
	.top-line nav ul li a
		font-size 14px
		padding 0 5px
	.top-line nav a.cb
		font-size 12px
	.top-line .top-phone div
		font-size 22px
	.top-line .top-phone span
		font-size 14px
	section.content .wrap .rooms-container .room .room-data .about-room .info .additional-info
		margin-right 25px
		padding-top 0
	section.content .wrap .rooms-container .room .room-data .about-room .info .additional-info + .room-price
		margin-top 20px
	section.content .wrap .rooms-slider .slider-nav .left-arrow
		left -15px
	section.content .wrap .rooms-slider .slider-nav .right-arrow
		right -15px
@media screen and (max-width: 1280px)
	section.content .wrap .rooms-container .room .room-data .about-room .room-preview,
	section.content .wrap .rooms-container .room .room-data .about-room .info .additional-info
		width 350px
	section.content .wrap .rooms-container .room .room-data .about-room .room-equip ul li:not(:last-child)
		margin-bottom 0
	section.content .wrap .rooms-container .room .room-data .about-room .info .additional-info
		margin-right 0
	section.content .wrap .rooms-container .room .room-data .about-room .info .room-price .price span
		font-size 20px
	section.content .wrap .rooms-container .room .room-data .about-room .info .room-price .price b
		font-size 23px
	section.content .wrap .rooms-container .room .room-data .about-room .info .room-price .order-room span
